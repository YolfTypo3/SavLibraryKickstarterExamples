{
    "name": "yolftypo3/sav-filters-mvc",
    "description": "A set of filters (alphabetic, months, search, selectors, mini calendar...) to be used with extension created by SAV Library Kickstarter for the SAV Library Mvc.",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "authors": [
        {
        "name": "Laurent Foulloy",
        "email": "yolf.typo3@orange.fr",
        "role": "Developer"
        }
    ],
    "replace": {
        "sav_filters_mvc": "self.version",
        "typo3-ter/sav-filters-mvc": "self.version"
    },
    "require": {
        "typo3/cms-core": ">=8.0,<10.0"
    },
    "autoload": {
        "psr-4": {
            "YolfTypo3\\SavFiltersMvc\\": "Classes/"
        }
    }
}
