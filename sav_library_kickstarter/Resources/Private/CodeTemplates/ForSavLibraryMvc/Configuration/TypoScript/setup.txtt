
<sav:function name="removeEmptyLines"><f:alias map="{
    extensionName:  '{extension.general.1.extensionKey->sav:function(name:\'removeUnderscore\')}'
}">
plugin.tx_{extensionName} {
    view {
        templateRootPaths.10 = <![CDATA[{]]>$plugin.tx_{extensionName}.view.templateRootPath}
        partialRootPaths.10 = <![CDATA[{]]>$plugin.tx_{extensionName}.view.partialRootPath}
        layoutRootPaths.10 = <![CDATA[{]]>$plugin.tx_{extensionName}.view.layoutRootPath}
    }
    persistence {
        storagePid = {$plugin.tx_{extensionName}.persistence.storagePid}
    }
}

config.tx_extbase.persistence.classes {
<f:for each="{extension.newTables}" as="table" key="tableKey">
<f:alias map="{
  model:'{sav:buildTableName(shortName:table.tablename, extensionKey:extension.general.1.extensionKey, mvc:mvc)}'
}">
    {sav:mvc.buildModelName(tableName:model, extension:extension, removeFirstBackslash:1)} <![CDATA[{]]>
        mapping {
            tableName = {model}
            columns <![CDATA[{]]>
                crdate.mapOnProperty = crdate
                cruser_id.mapOnProperty = cruserId 
            }   
        }        
    }
</f:alias>
</f:for>
}
</f:alias>
</sav:function>